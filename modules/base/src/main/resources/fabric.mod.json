{
  "schemaVersion": 1,
  "id": "porting_lib_base",
  "version": "${version}",
  "name": "Porting Lib Base",
  "description": "Assorted utilities for porting mods from Forge to Fabric, awaiting further sorting into modules.",
  "entrypoints": {
    "main": [
      "io.github.fabricators_of_create.porting_lib.PortingLibBase"
    ],
    "client": [
      "io.github.fabricators_of_create.porting_lib.PortingLibClient"
    ]
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_2680": ["io/github/fabricators_of_create/porting_lib/extensions/BaseBlockStateExtension"],
      "net/minecraft/class_332": ["io/github/fabricators_of_create/porting_lib/extensions/GuiGraphicsExtension"]
    }
  }
}
